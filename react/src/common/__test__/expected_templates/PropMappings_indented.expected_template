const figma = require('figma')

const variant = figma.properties.enum('ğŸ‘¥ Variant', {
"Primary": 'primary',
"Destructive": 'destructive',
"Inverse": 'inverse',
"Success": 'success',
"FigJam": 'FigJam',
"Secondary": 'secondary',
"Secondary Destruct": 'destructive-secondary'})
const size = figma.properties.enum('ğŸ‘¥ Size', {
"Default": 'hug-contents',
"Large": undefined,
"Wide": 'fit-parent'})
const disabled = figma.properties.boolean('ğŸ›ï¸ Disabled')
const iconLead = figma.properties.boolean('ğŸ›ï¸ Icon Lead', {
"true": 'icon',
"false": undefined})
const label = figma.properties.string('ğŸ›ï¸ Label')

export default figma.tsx`<Button${_fcc_renderReactProp('variant', variant)} onClick={() => { }}${_fcc_renderReactProp('width', size)}${_fcc_renderReactProp('disabled', disabled)}${_fcc_renderReactProp('iconLead', iconLead)}>
        ${label}
      </Button>`
